{
  "name": "easyui-jsx",
  "author": "James Smith",
  "version": "1.0.12",
  "license": "MIT",
  "homepage": "https://github.com/djalbat/EasyUI-JSX",
  "description": "JSX support for EasyUI.",
  "repository": {
    "type": "git",
    "url": "https://github.com/djalbat/EasyUI-JSX"
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browserify": "^13.0.0",
    "chokidar-cli": "^1.2.0",
    "easyui": "^7.1.30",
    "easyui-draganddrop": "^5.0.9",
    "easyui-layout": "^1.0.29",
    "easyui-richtextarea": "^1.0.14",
    "fragmented": "^1.0.6"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "babel": "babel ./es6 --presets=es2015,react --plugins transform-react-jsx --out-dir=./lib",
    "babel-debug": "babel ./es6 --presets=es2015,react --plugins transform-react-jsx --source-maps inline --out-dir=./lib",
    "browserify:examples": "browserify ./examples/index.js --standalone examples > ./docs/examples.js",
    "browserify:examples-debug": "browserify ./examples/index.js --standalone examples --debug > ./docs/examples.js",
    "build": "npm run clean && npm run babel && npm run browserify:examples",
    "build-debug": "npm run babel-debug && npm run browserify:examples-debug",
    "watch": "npm run build && chokidar './index.js' './examples/index.js' './es6/**/*.js' -c 'npm run build'",
    "watch-debug": "npm run build-debug && chokidar './index.js' './examples/index.js' './es6/**/*.js' -c 'npm run build-debug'"
  }
}
